#include<iostream>
#include<vector>
using namespace std;
void merge(vector<int> &arr,int str,int mid,int end){
    vector<int>temp;
    int i=str,j=mid+1;
    while(i<=mid&&j<=end){
        if(arr[i]<arr[j]){
            temp.push_back(arr[i]);
            i++;
        }
        else{
            temp.push_back(arr[j]);
            j++;
        }
    }
    while(i<=mid){
        temp.push_back(arr[i]);
        i++;
    }
    while(j<=end){
        temp.push_back(arr[j]);
        j++;
    }
    for(int i=0;i<temp.size();i++){
        arr[i+str]=temp[i];
    }
}
void mergesort(vector<int> &arr,int str,int end){
    if(str<end){
    int mid=str+(end-str)/2;
    mergesort(arr,str,mid);
    mergesort(arr,mid+1,end);
    merge(arr,str,mid,end);
}
}
int main(){
    vector<int> arr={6,4,5,3,2,1};
    mergesort(arr,0,5);
    for(int i=0;i<6;i++){
        cout<<arr[i]<<" ";
    }
}
